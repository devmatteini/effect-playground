{
  "name": "effect-playground",
  "version": "1.0.0",
  "description": "A collection of scripts to explore the Effect ecosystem",
  "author": "Cosimo Matteini",
  "scripts": {
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "typecheck:w": "tsc --noEmit --watch",
    "test": "vitest run",
    "test:w": "vitest watch"
  },
  "dependencies": {
    "effect": "2.4.13",
    "@effect/cli": "0.35.19",
    "@effect/platform": "0.48.17",
    "@effect/platform-node": "0.45.19",
    "@effect/schema": "0.64.13"
  },
  "devDependencies": {
    "@types/node": "18.18.6",
    "typescript": "5.4.3",
    "tsx": "4.4.0",
    "prettier": "3.1.0",
    "@effect/vitest": "0.2.1",
    "vitest": "1.4.0",
    "fast-check": "3.17.0"
  }
}
